{
    "env": {
        "browser": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "brace-style": "off",
        "comma-dangle": "off",
        "consistent-return": "off",
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ],
        "no-underscore-dangle": "off"
    }
}
